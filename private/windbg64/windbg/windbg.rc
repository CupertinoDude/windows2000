    POPUP "&Help"
    BEGIN
        MENUITEM "&Contents",                       IDM_HELP_CONTENTS
        MENUITEM "&Search",                         IDM_HELP_SEARCH
        MENUITEM SEPARATOR
        MENUITEM "&About...",                       IDM_HELP_ABOUT
    END
END

