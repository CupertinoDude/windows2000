        MENUITEM "Colors...",                       IDM_VIEW_COLORS
        MENUITEM SEPARATOR
        MENUITEM "&Options...",                     IDM_VIEW_OPTIONS
    END
    POPUP "&Debug"
    BEGIN
        MENUITEM "&Go\tF5",                         IDM_DEBUG_GO
        MENUITEM "&Restart\tCtrl+Shift+F5",         IDM_DEBUG_RESTART
        MENUITEM "Stop &Debugging\tShift+F5",       IDM_DEBUG_STOPDEBUGGING
