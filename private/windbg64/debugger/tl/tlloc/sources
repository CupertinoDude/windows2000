!INCLUDE ..\..\sources.inc


386_STDCALL=1
USE_NATIVE_EH=0

MAJORCOMP=dbg
MINORCOMP=tlloc

TARGETNAME=tlloc$(D)
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLDEF=tlloc.def
COFFBASE=tlloc

DLLENTRY=DllMain
UMTYPE=windows
SOURCES=    tllp.c  \
            tlloc.rc


C_DEFINES= $(C_DEFINES) -DADDR_MIXED -DOSDEBUG4 -DDOLPHIN

INCLUDES=$(LANGAPI_ROOT)\debugger;\
	$(DEBUGGER_ROOT)\include;\
	$(DEBUGGER_ROOT)\od\include;\
	$(DEBUG_INCLUDES)


TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\user32.lib
