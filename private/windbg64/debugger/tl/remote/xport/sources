!INCLUDE ..\..\..\sources.inc

MAJORCOMP=dbg
MINORCOMP=xport

TARGETNAME=xport
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES=xport.c

UMTYPE=windows

C_DEFINES=$(C_DEFINES) -DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED

INCLUDES=$(LANGAPI_ROOT)\debugger;\
        $(DEBUGGER_ROOT)\include;\
        $(DEBUGGER_ROOT)\od\include;\
        $(DEBUG_INCLUDES)

