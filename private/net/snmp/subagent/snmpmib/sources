    $(BASEDIR)\public\sdk\inc;  \
    $(BASEDIR)\private\net\inc; \
    ..\..\newagent\inc

SOURCES=\
    dllmain.c   \
    mibentry.c  \
    mibfuncs.c  \
    snmpmib.rc

