DLLBASE=0x1000000

MAJORCOMP=ntsd
MINORCOMP=ntsdexts

TARGETNAME=regexts
TARGETPATH=\nt\public\sdk\lib
TARGETTYPE=DYNLINK
TARGETLIBS=\nt\public\sdk\lib\*\kernel32.lib
DLLDEF = regexts.def
MSC_WARNING_LEVEL=/WX

USE_NTDLL=1

INCLUDES=       .;..

C_DEFINES=-DNTENV -DWIN32RPC -DNTWIN32RPC -DMSWMSG -D_RPCRT4_

SOURCES=regexts.cxx     \
        regexts.rc

UMTYPE=console
