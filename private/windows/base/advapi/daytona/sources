         ..\regdat16.c  \
         ..\cseclogn.cxx \
         ..\sitesids.cxx \
         ..\efs.c       \
         ..\sddl.c      \
         ..\termutil.c

UMTYPE=console
OPTIONAL_UMTEST=tuser

