MAJORCOMP=wmi
MINORCOMP=wmistub

TARGETNAME=wmi
TARGETTYPE=DYNLINK
TARGETPATH=$(BASEDIR)\public\sdk\lib

LINKLIBS= \
          $(BASEDIR)\public\sdk\lib\*\advapip.lib

DLLDEF=obj\*\wmi.def

!IFNDEF MSC_WARNING_LEVEL
MSC_WARNING_LEVEL=/W3
!ENDIF
MSC_WARNING_LEVEL=$(MSC_WARNING_LEVEL) /WX 

NTTARGETFILES=$(TARGETPATH)\*\wmip.lib

SOURCES=wmi.rc

UMTYPE=windows

