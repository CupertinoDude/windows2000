MAJORCOMP=install
MINORCOMP=EXE

TARGETNAME=capiinst
TARGETPATH=..\bin
TARGETTYPE=PROGRAM

INCLUDES=..\..\inc;

TARGETLIBS=\
!if "$(ACT_BUILD)" == "TRUE"
        $(BASEDIR)\private\security\lib\*\capi.lib\
        $(BASEDIR)\public\sdk\lib\*\advapi32.lib
!else
        $(BASEDIR)\public\sdk\lib\*\advapi32.lib
!endif


SOURCES=\
install.c   \
install.rc

UMTYPE=console
UMENTRY=main

USE_MSVCRT=1
