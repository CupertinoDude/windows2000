    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               IDM_EDIT_UNDO, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Copy\tCtrl+C",               IDM_EDIT_COPY
