MAJORCOMP=    accesory
MINORCOMP=    write

TARGETNAME=   write

TARGETPATH=   obj

TARGETTYPE=   PROGRAM

USE_MSVCRT=1

SOURCES=      \
              write.rc \
              write.c

UMTYPE=       windows
UMENTRY=      winmain

UMLIBS=       \
              $(BASEDIR)\public\sdk\lib\*\shell32.lib  \
              $(BASEDIR)\public\sdk\lib\*\kernel32.lib
