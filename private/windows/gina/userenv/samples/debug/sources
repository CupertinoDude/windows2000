MAJORCOMP=windows
MINORCOMP=shell

TARGETNAME=debug
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=

C_DEFINES= -DWIN32 -DUNICODE -D_UNICODE

SOURCES=debug.c \
	debug.rc

UMTYPE=windows
UMENTRY=winmain
UMAPPL=debug
UMLIBS=\
       obj\*\debug.lib \
       obj\*\debug.res
