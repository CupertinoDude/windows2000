# On NT we always copy the lib to sdk\lib\* because danim needs to link to it
!if "$(BUILD_PRODUCT)" == "NT"
TARGETPATHLIB=$(BASEDIR)\public\sdk\lib
!else
!if $(FREEBUILD)
TARGETPATHLIB=$(BASEDIR)\public\sdk\lib
!else
TARGETPATHLIB=$(_OBJ_DIR)
!endif
!endif

