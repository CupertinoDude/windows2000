    iactivescript.cxx \
    iactivescripterror.cxx \
    iactivescriptparse.cxx \
    iactivescriptsite.cxx 


INCLUDES= \
        $(ROOT); \
        $(ROOT)\include; \
        $(ROOT)\core\base; \
        $(ROOT)\core\com; \
        $(IDL_GENERATED_HEADERS); \
        $(WINDOWS_INC_PATH)

!ifdef WIN16
PRECOMPILED_OPTION=/fhr /fh=$(ROOT)\include\$(O)\core.pch
!else
PRECOMPILED_OPTION=/Yucore.hxx /Fp$(ROOT)\include\$(O)\core.pch
!endif
PRECOMPILED_CXX=1


