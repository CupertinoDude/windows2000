; Other keys that we nuke when we get unregistered
HKCU,%XMLParser_PATH%,,,

[Strings]
CLSID_XMLParser ="{3aaa7326-09ee-11d2-9cab-0060b0ec3d39}"
TypeLibID = "{3aaa7322-09ee-11d2-9cab-0060b0ec3d39}"
