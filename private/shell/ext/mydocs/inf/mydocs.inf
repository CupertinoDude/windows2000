AddReg=mydocs.reg


[mydocs.x86]
mydocs.dll,mydocs.x86

[mydocs.alpha]
mydocs.dll,mydocs.alpha


; Uninstall - remove the DLL and the registry entries
[DefaultUninstall]
DelFiles=mydocs.inf, mydocs.dll
