    <SCRIPT LANGUAGE="vbscript">
        Const g_nCR = 13
        Const g_nLF = 10

        Function TranslateCRs(strIn)
        Dim strOut
        strOut = ""

        While 0 < Len(strIn)
            if Asc(strIn) = g_nCR then
                strIn = strIn & " "
                strIn = Right(strIn, Len(strIn) - 1)
            else
                strOut = strOut & Chr(Asc(strIn))
            end if

            strIn = Right(strIn, Len(strIn) - 1)
        Wend

        TranslateCRs = strOut
        end Function
    </SCRIPT>

