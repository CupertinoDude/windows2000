[version]
signature="$CHICAGO$"
SetupClass=BASE

[DefaultInstall]
CopyFiles=shell.sys.install,shell.win.install

[shell.sys.install]
shell32.dll
shell32.sym
shlwapi.dll
shlwapi.sym
comctl32.dll
comctl32.sym
shdocvw.dll
shdocvw.sym
shlwapi.dll
shlwapi.sym

[shell.win.install]
explorer.exe,explorer.exe
