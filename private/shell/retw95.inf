[SourceDisksNames]
55="Shell32","",1,       shell32\win95\obj\i386
57="ComCtl32","",1,     comctl32\win95\obj\i386
59="Explorer","",1,     explorer\win95\obj\i386
60="SHDocVw","",1,         shdocvw\dll\obj\i386
61="SHLWAPI","",1,         shlwapi\dll\obj\i386

[SourceDisksFiles]
shell32.dll=55
shell32.sym=55

comctl32.dll=57
comctl32.sym=57

explorer.exe=59
explorer.sym=59

shdocvw.dll=60
shdocvw.sym=60

shlwapi.dll=61
shlwapi.sym=61

[Strings]
